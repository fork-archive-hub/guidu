{
  "name": "@uidu/inputs",
  "version": "1.0.0-beta.31",
  "description": "Inputs using Formsy",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uidu-org/uidu-ui-kit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "lib",
    "styles"
  ],
  "scripts": {
    "dev": "webpack --config webpack.development.js --progress --colors --watch",
    "build": "npm run build:es && npm run build:css",
    "build:umd": "webpack --config webpack.production.js",
    "prebuild:css": "node-sass --include-path ../../node_modules styles/index.scss styles/index.css",
    "build:css": "postcss --use autoprefixer --config postcss.config.js styles/index.css -o styles/index.css",
    "build:es": "NODE_ENV=production cross-env BABEL_ENV=es babel src --out-dir lib",
    "lint": "eslint src/**; exit 0",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "test": "bundlesize",
    "storybook": "start-storybook --ci -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "start": "webpack-dev-server --open --config webpack.development.js --progress --colors --watch"
  },
  "author": "",
  "license": "ISC",
  "bundlesize": [
    {
      "path": "./dist/umd/*.js",
      "maxSize": "300 kB"
    }
  ],
  "dependencies": {
    "@uidu/loader": "^1.0.0-beta.3",
    "antd": "^3.0.1",
    "autosize": "^4.0.0",
    "blueimp-load-image": "^2.17.1",
    "classnames": "^2.2.5",
    "cleave.js": "^1.3",
    "downshift": "^3.0.0",
    "formsy-react": "^1.0.0",
    "moment": "^2.18.1",
    "pell": "^1.0.4",
    "react": "^15.6.1 || ^16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-avatar-editor": "11.0.4",
    "react-color": "^2.13.8",
    "react-day-picker": "^7.0.2",
    "react-dom": "^15.6.1 || ^16.2.0",
    "react-feather": "^1.0.7",
    "react-keyboard-time-input": "^2.1.1",
    "react-mentions": "^2.0.1",
    "react-numeric-input": "^2.2.0",
    "react-select": "^2.0.0",
    "react-spinkit": "^3.0.0",
    "react-switch": "^3.0.2",
    "sass-loader": "^7.0.2",
    "shortid": "^2.2.8",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "@uidu/docs": "^0.1.0",
    "@uidu/forms": "^1.0.0-beta.8",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4"
  },
  "gitHead": "4abc79f75583da9a7768ae84abd15cec1cc2b82d"
}
