{
  "name": "@uidu/editor",
  "version": "1.0.0-beta.12",
  "description": "WYSIWYG Editor",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/uidu-org/uidu-ui-kit.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "lib",
    "styles"
  ],
  "scripts": {
    "dev": "webpack --config webpack.development.js --progress --colors --watch",
    "build": "npm run build:es",
    "build:umd": "webpack --config webpack.production.js",
    "build:es": "NODE_ENV=production cross-env BABEL_ENV=es babel src --out-dir lib",
    "lint": "eslint src/**; exit 0",
    "lint:watch": "esw -w src/**",
    "prepublish": "npm run build",
    "test": "bundlesize",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "start": "webpack-dev-server --open --config webpack.development.js --progress --colors --watch"
  },
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "300 kB"
    }
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ckeditor/ckeditor5-adapter-ckfinder": "^10.0.2",
    "@ckeditor/ckeditor5-autoformat": "^10.0.2",
    "@ckeditor/ckeditor5-basic-styles": "^10.0.2",
    "@ckeditor/ckeditor5-block-quote": "^10.0.2",
    "@ckeditor/ckeditor5-core": "^11.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^11.0.0",
    "@ckeditor/ckeditor5-easy-image": "^10.0.2",
    "@ckeditor/ckeditor5-editor-classic": "^11.0.0",
    "@ckeditor/ckeditor5-engine": "^11.0.0",
    "@ckeditor/ckeditor5-essentials": "^10.1.1",
    "@ckeditor/ckeditor5-heading": "^10.0.2",
    "@ckeditor/ckeditor5-image": "^11.0.0",
    "@ckeditor/ckeditor5-link": "^10.0.3",
    "@ckeditor/ckeditor5-list": "^11.0.1",
    "@ckeditor/ckeditor5-paragraph": "^10.0.2",
    "@ckeditor/ckeditor5-react": "^1.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^11.0.0",
    "@ckeditor/ckeditor5-ui": "^11.0.0",
    "@ckeditor/ckeditor5-upload": "^10.0.2",
    "ckeditor5-embed": "^1.0.3",
    "get-youtube-id": "^1.0.0",
    "react-feather": "^1.0.7",
    "react-transition-group": "^2.2.1"
  },
  "devDependencies": {
    "@uidu/docs": "^0.1.0",
    "webpack-cli": "^3.1.2"
  }
}
