{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": [
    "prettier",
    "compat",
    "react",
    "jsx-a11y",
    "import",
    "class-property"
  ],
  "rules": {
    "function-paren-newline": "off",
    "arrow-parens": "off",
    "compat/compat": "error",
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "import/extensions": "off",
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
